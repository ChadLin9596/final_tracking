<launch>
    <param name="/use_sim_time" value="true"/>
    <include file="$(find final_tracking)/launch/tf_gpal.launch"/>
<!--    <node pkg="final_tracking" type="point_raw_transform" name="point_raw_transfrom"/>

    <node pkg="final_tracking" type="cluster_cloud" name="cluster_cloud" output="screen"/>-->
    <node pkg="final_tracking" type="predict_odometry" name="predict_odometry" output="screen"/>

    <!-- visualization -->
    <arg name="isRviz" default="false"/>
    <group if="$(arg isRviz)">
        <node launch-prefix="nice" pkg="rviz" type="rviz" name="rviz"
        args="-d $(find final_tracking)/rviz_cfg/final_localization.rviz" />
    </group>
</launch>
